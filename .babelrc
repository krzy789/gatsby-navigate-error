{
  "presets": [
    "babel-preset-gatsby",
    "@babel/preset-react",
    ["@babel/preset-env", {
      "shippedProposals": true,
      "useBuiltIns": "entry",
      "corejs": 3,
      "loose": true
    }]
  ],
  "plugins": [
    [
      "@babel/plugin-proposal-class-properties",
      {
        "loose": true
      }
    ]
  ],
  "env": {
    "test": {
      "plugins": ["require-context-hook"]
    }
  }
}
